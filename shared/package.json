{
  "name": "@bjanderson/my-project-shared",
  "version": "0.0.1",
  "author": "BJ Anderson",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm rm @bjanderson/my-project-shared -g && rimraf dist",
    "build": "tsc",
    "postbuild": "npm link",
    "fix-ts-paths": "node scripts/fix-ts-paths.js",
    "doc": "typedoc --out ./docs/utils src/**/*.ts",
    "lint": "eslint ./src --ext .ts",
    "prepublish": "npm run build",
    "push:major": "npm version major",
    "push:minor": "npm version minor",
    "push:patch": "npm version patch",
    "start": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --clearCache && jest --coverage --watch"
  },
  "devDependencies": {
    "@bjanderson/utils": "3.1.0",
    "@types/jest": "29.5.12",
    "@types/luxon": "3.4.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/eslint-plugin-tslint": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "import": "0.0.6",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "tslint": "5.20.1",
    "typedoc": "0.25.8",
    "typescript": "5.3.3"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@bjanderson/utils": "3.1.0"
  },
  "dependencies": {
    "luxon": "3.4.4"
  }
}
