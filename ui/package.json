{
  "name": "angular-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "sass": "sass --load-path ./node_modules --load-path ./src/styles --load-path ./ --no-source-map",
    "sass:material": "npm run sass src/material.scss ./tmp/material.css",
    "sass:styles": "npm run sass src/styles.scss ./tmp/styles.css",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@bjanderson/utils": "3.1.0",
    "@fortawesome/fontawesome-free": "6.5.2",
    "luxon": "3.4.4",
    "ngx-toastr": "19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "9.0.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jest": "29.5.12",
    "@types/luxon": "3.4.2",
    "@types/uuid": "9.0.8",
    "jest": "29.7.0",
    "jest-preset-angular": "14.1.0",
    "typescript": "~5.4.2"
  }
}
